{
    "manifest_version": 3,
    "name": "Yabla Complete",
    "description": "Automatically complete yabla multiple choice activity and fill in the blank activity, and download transcripts of videos.",
    "version": "1.3.0",
    "web_accessible_resources": [
        {
            "resources": ["js/autocomplete.js"],
            "matches": ["https://*.yabla.com/*"]
        }
    ],

    "permissions": ["contextMenus", "tabs"],
    "content_scripts": [
        { "matches": ["https://*.yabla.com/*"], "js": ["js/inject.js"] }
    ]
}
